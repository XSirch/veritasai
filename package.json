{
  "name": "veritas-ai-extension",
  "version": "1.0.0",
  "description": "Extensão de navegador para verificação de confiabilidade de informações",
  "main": "src/background/background.js",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:integration": "jest tests/integration",
    "test:e2e": "playwright test",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src tests --ext .js",
    "lint:fix": "eslint src tests --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\"",
    "setup": "node scripts/setup-dev.js && python scripts/setup_python.py",
    "setup:python": "python scripts/setup_python.py",
    "docker:check": "node scripts/check-docker.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "docker:test:up": "docker-compose -f docker/docker-compose.test.yml up -d",
    "docker:test:down": "docker-compose -f docker/docker-compose.test.yml down",
    "docker:test:clean": "docker-compose -f docker/docker-compose.test.yml down -v --remove-orphans",
    "qdrant:setup": "node scripts/setup-qdrant.js",
    "qdrant:status": "curl -f http://localhost:6333/health || echo 'Qdrant não está rodando'",
    "qdrant:dashboard": "open http://localhost:6333/dashboard",
    "package": "node scripts/package.js",
    "clean": "rimraf dist coverage test-results",
    "precommit": "npm run lint && npm run test:unit",
    "prepare": "husky install",
    "py:test": "uv run pytest",
    "py:test:watch": "uv run pytest-watch",
    "py:test:cov": "uv run pytest --cov=src --cov-report=html",
    "py:lint": "uv run python scripts/lint.py",
    "py:format": "uv run python scripts/format.py",
    "py:sync": "uv sync",
    "py:add": "uv add",
    "py:remove": "uv remove",
    "validate": "node scripts/validate-manifest.js",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:py": "uv run pytest",
    "test:py:coverage": "uv run pytest --cov=src --cov-report=html",
    "coverage": "node scripts/coverage-report.js",
    "coverage:open": "npm run coverage && open coverage-report/index.html"
  },
  "dependencies": {
    "@qdrant/js-client-rest": "^1.15.0",
    "@xenova/transformers": "^2.17.0",
    "compromise": "^14.14.4",
    "compromise-dates": "^3.7.1",
    "compromise-numbers": "^1.4.0",
    "crypto-js": "^4.2.0",
    "idb": "^7.1.1",
    "lodash.debounce": "^4.0.8",
    "natural": "^8.1.0",
    "stopword": "^3.1.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@playwright/test": "^1.40.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.1.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/veritas-ai-extension.git"
  },
  "keywords": [
    "fact-check",
    "browser-extension",
    "ai",
    "misinformation",
    "verification",
    "chrome-extension",
    "firefox-addon"
  ],
  "author": "VeritasAI Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-username/veritas-ai-extension/issues"
  },
  "homepage": "https://github.com/your-username/veritas-ai-extension#readme"
}
